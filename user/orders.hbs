<div class="container">
   
<table class="table mt-5">
    <thead>
        <tr>
            <th scope="col">Date</th>
            <th>Address</th>
            <th>Pincode</th>
            <th>Mobile</th>
            <th scope="col">Amount</th>
            <th scope="col">Peyment</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        {{#each orders}}
            <tr>
                <td>{{this.date}}</td>
                <td>{{this.deliveryDetails.address}}</td>
                <td>{{this.deliveryDetails.pincode}}</td>
                <td>{{this.deliveryDetails.mobile}}</td>
                <td>{{this.deliveryDetails.totalAmount}}</td>
                <td>{{this.deliveryDetails.peymentMethod}}</td>
                <td>{{this.deliveryDetails.status}}</td>
            </tr>

        {{/each}}
    </tbody>

</table>

</div>